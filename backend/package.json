{
    "name": "memotopia-api-backend",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "yarn run task:prettier:fix",
        "start": "yarn start:tsnode:dev",
        "start:tsnode": "cross-env API_ENVIRONMENT=local ts-node --transpile-only -r tsconfig-paths/register src/main.ts",
        "start:tsnode:dev": "cross-env API_ENVIRONMENT=local ts-node-dev -r tsconfig-paths/register src/main.ts",
        "start:dev": "cross-env API_ENVIRONMENT=local tsc-watch -p tsconfig.build.json --onSuccess \"node -r ./tsconfig-paths-bootstrap.js dist/src/main.js\"",
        "start:debug": "cross-env API_ENVIRONMENT=local tsc-watch -p tsconfig.json --onSuccess \"node -r ./tsconfig-paths-bootstrap.js --inspect-brk dist/main.js\"",
        "start:prod": "cross-env API_ENVIRONMENT=local NODE_ENV=dev node -r ./tsconfig-paths-bootstrap.js  dist/src/main.js",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "cross-env API_ENVIRONMENT=test NODE_ENV=test jest --runInBand --testTimeout 99999999",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "cross-env API_ENVIRONMENT=test jest --config ./test/jest-e2e.json",
        "db:reset": "docker-compose down -v && docker-compose up",
        "db:start": "docker-compose --env-file .env.local up",
        "db:test": "docker-compose --env-file .env.test up",
        "docker:build": "docker build -t memotopia-api-backend-api:$npm_package_version .",
        "task:typecheck": "tsc --noEmit -p tsconfig.build.json",
        "task:lint:fix": "eslint . --ext .ts --fix",
        "task:prettier:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" --loglevel error",
        "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ormconfig.ts",
        "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\""
    },
    "dependencies": {
        "@nestjs-modules/mailer": "^1.6.0",
        "@nestjs/common": "^7.4.0",
        "@nestjs/config": "^0.5.0",
        "@nestjs/core": "^7.4.0",
        "@nestjs/jwt": "^7.2.0",
        "@nestjs/passport": "^7.1.5",
        "@nestjs/platform-express": "^7.4.0",
        "@nestjs/swagger": "^4.7.9",
        "@nestjs/typeorm": "^7.1.1",
        "@sentry-internal/typescript": "^6.13.2",
        "@sentry/integrations": "^6.13.2",
        "@sentry/node": "^6.13.2",
        "@sentry/tracing": "^6.13.2",
        "@types/inline-css": "^3.0.0",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/lokijs": "^1.5.3",
        "@types/nodemailer": "^6.4.2",
        "@types/request-ip": "^0.0.35",
        "@types/uuid": "^8.3.0",
        "apple-signin-auth": "^1.4.1",
        "aws-sdk": "^2.738.0",
        "bcryptjs": "^2.4.3",
        "cognito-express": "^2.0.18",
        "copyfiles": "^2.3.0",
        "cross-env": "^7.0.2",
        "handlebars": "^4.7.7",
        "jest-junit": "^11.1.0",
        "json-typescript": "^1.1.2",
        "jsonwebtoken": "^8.5.1",
        "lokijs": "^1.5.11",
        "nodemailer": "^6.6.1",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "pg": "^8.3.2",
        "reflect-metadata": "^0.1.13",
        "request-ip": "^2.1.3",
        "rimraf": "^3.0.2",
        "rxjs": "^6.5.4",
        "short-uuid": "^3.1.1",
        "swagger-ui-express": "^4.1.6",
        "ts-node-dev": "^1.0.0-pre.59",
        "tsc-watch": "^4.2.9",
        "typeorm": "^0.2.25",
        "uuid": "^8.3.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.4.0",
        "@nestjs/schematics": "^7.1.1",
        "@nestjs/testing": "^7.4.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/express": "^4.17.3",
        "@types/jest": "25.2.3",
        "@types/morgan": "^1.9.1",
        "@types/node": "^13.9.1",
        "@types/passport-jwt": "^3.0.3",
        "@types/pg": "^7.14.4",
        "@types/supertest": "^2.0.8",
        "@typescript-eslint/eslint-plugin": "3.0.2",
        "@typescript-eslint/parser": "3.0.2",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.12.2",
        "eslint": "7.1.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
        "jest": "26.0.1",
        "morgan": "^1.10.0",
        "prettier": "^1.19.1",
        "supertest": "^4.0.2",
        "ts-jest": "26.1.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^9.0.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.7.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "moduleNameMapper": {
            "^~(.*)": "<rootDir>/$1"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
